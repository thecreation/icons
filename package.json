{
  "name": "standard-icons",
  "workspaces": [
    "modules/*",
    "tools/*"
  ],
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "import": "lerna run import",
    "optimize": "lerna run optimize",
    "clean": "lerna clean",
    "update": "lerna run --parallel update",
    "distclean": "lerna clean --yes && rm -rf node_modules",
    "diff": "lerna diff",
    "updated": "lerna updated || exit 0",
    "lint": "lerna run lint",
    "release": "scripts/global/release",
    "publish-skipgit": "lerna publish --skip-git",
    "bump": "lerna publish --exact --skip-npm --since \"v$(npm info icon version)\"",
    "new-module": "scripts/global/new-module"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-external-helpers": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.5",
    "colors": "^1.4.0",
    "css": "^3.0.0",
    "cssnano": "^4.1.10",
    "cssom": "^0.4.4",
    "del": "^6.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-compat": "^3.9.0",
    "extend": "^3.0.2",
    "extract": "^1.0.0",
    "fs-extra": "^9.1.0",
    "gaze": "^1.1.3",
    "glob-expand": "^0.2.1",
    "glob-parent": "^5.1.1",
    "globby": "^11.0.2",
    "globule": "^1.3.2",
    "graceful-fs": "^4.2.4",
    "handlebars": "^4.7.6",
    "humanize-string": "^2.1.0",
    "husky": "^4.3.8",
    "install": "^0.13.0",
    "js-yaml": "^4.0.0",
    "jsonfile": "^6.1.0",
    "jsonlint": "^1.6.3",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.3",
    "lodash.indexby": "^3.1.1",
    "mkdirp": "^1.0.4",
    "node-sass": "^5.0.0",
    "node-sass-import": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rename": "^1.0.4",
    "requestretry": "^4.1.2",
    "retry-request": "^4.1.3",
    "semver": "^7.3.4",
    "svgicons2svgfont": "^9.1.1",
    "svgo": "^1.3.2",
    "webfont": "^9.0.0",
    "xml2js": "^0.4.23",
    "yargs": "^16.2.0",
    "yargs-parser": "^20.2.4",
    "zlib": "^1.0.5"
  }
}
